{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext", "ES6"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitThis": true,
    "baseUrl": "./",
    "paths": {
      "@cornerstonejs/streaming-image-volume-loader": [
        "src/utils/streaming-image-volume-loader/src"
      ],
      "@cornerstonejs/core": ["src/utils/core/src"],
      "@cornerstonejs/tools": ["src/utils/tools/src"],
      "@src/*": ["src/*"]
    }
  },
  "include": ["src"],
  "references": [{
    "path": "./tsconfig.node.json"
  }]
}